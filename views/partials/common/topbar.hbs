<div class="header">
    <div class="header-left">
        {{#compare userSession.role '==' 'superAdmin'}}
        <div class="menu-icon bi bi-list"></div>
        {{/compare}}

    </div>
    <div class="header-right">
        <div class="user-info-dropdown">
            <div class="dropdown">
                <a class="dropdown-toggle" href="#" role="button" data-toggle="dropdown">
                    <span class="user-icon">
                        <img src="/vendors/images/photo1.jpg" alt="" />
                    </span>
                    <span class="user-name">{{userSession.name}}</span>
                </a>
                <div class="dropdown-menu dropdown-menu-right dropdown-menu-icon-list">
                    <a class="dropdown-item" href="/users/profile"><i class="dw dw-user1"></i>
                        Profile</a>
                    <a class="dropdown-item" href="/logout"><i class="dw dw-logout"></i> Log Out</a>
                </div>
            </div>
        </div>
        <div class="github-link">
            <a href="javascript:void(0);"><img src="/vendors/images/github.svg" alt="" /></a>
        </div>
    </div>
</div>